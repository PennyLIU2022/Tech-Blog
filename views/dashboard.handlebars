<h2>Your Posts</h2>

<button type="submit" class="btn new-post-button"><a class="link" href="/dashboard/new">+ New Post</a></button>
<section>
{{#if blogs.length}}
   
    {{#each blogs as |blog|}}

      {{> dashboard-postinfo blog}}
      {{> comments blog.comments}}
    {{/each}}

{{/if}}
</section>

<script src="/js/add-blog.js"></script>